def Sorting(lst):
    lst2 = sorted(lst, key=len)
    return lst2

num= int(input("Enter Number of inputs"))
lst=[]
if num >=1 :
    for n in range(num+1):
        inp=input()
        lst.append(inp)
    sorted_list = Sorting(lst)
    print(sorted_list)
    print(sorted_list[0])

else:
    print("Invalid Input")
